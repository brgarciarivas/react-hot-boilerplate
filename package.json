{
  "name": "react-hot-boilerplate",
  "version": "1.0.0",
  "description": "A starter project for getting up and running with react, redux, webpack, and hot reloading",
  "main": "server.js",
  "scripts": {
    "start": "forever stopall; forever start server.js",
    "stop": "forever stopall",
    "deploy": "npm run build; npm start",
    "build": "NODE_ENV=production webpack --progress --colors -p",
    "test": "webpack-dev-server --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krefsar/react-hot-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "hot"
  ],
  "author": "krefsar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/krefsar/react-hot-boilerplate/issues"
  },
  "homepage": "https://github.com/krefsar/react-hot-boilerplate#readme",
  "dependencies": {
    "babel-runtime": "^6.5.0",
    "body-parser": "^1.14.2",
    "express": "^4.13.4",
    "forever": "^0.15.1",
    "history": "^2.0.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0",
    "react-tap-event-plugin": "^0.2.2"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-runtime": "^6.5.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
